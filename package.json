  },
  "scripts": {
    "build:dev": "npm run clean-build && cross-env NODE_ENV=development webpack --profile --progress --env development",
    "build:prod": "npm run clean-build && cross-env NODE_ENV=production webpack --profile --progress --env production",
    "clean-build": "rm -rf ./build/*",
    "clean-dll": "rm -rf ./dll/*",
    "dev": "npm run clean-build && cross-env NODE_ENV=hot webpack-dev-server --progress --env development",
    "dll": "npm run clean-dll && webpack --profile --progress --env development --config config/webpack.config.vendor.js",
    "fix:css": "stylelint 'src/**/*.css' --fix",
    "fix:js": "prettier --single-quote --trailing-comma es5 --write '**/*.js'",
    "fix:ts": "prettier --parser typescript --single-quote --trailing-comma es5 --write 'src/**/*.{ts,tsx}'",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint '**/*.js' --ignore-path .gitignore",
    "lint:ts": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}'",
    "postinstall": "npm run dll",
    "test": "karma start config/karma.conf.js"
  },
  "dependencies": {
